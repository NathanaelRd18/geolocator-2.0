import os
import sys
import glob, codecs

def main():
	curdir = os.getcwd();
	filedir = curdir + "/sgm/*.sgm";
	filepath = glob.glob(filedir);
	for filename in filepath:
		with codecs.open(filename, 'r', encoding = 'UTF-8') as infile:
			for line in infile.readlines():
				l1 = line.find('<');
				l2 = line.find('>', l1);
				print line[l1: l2];
if __name__=="__main__":
	main();